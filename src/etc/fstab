LABEL=Main\040Data /home/ides3rt btrfs nosuid,nodev,noatime,compress-force=zstd:1,space_cache=v2 0 2

tmpfs /tmp tmpfs nosuid,nodev,noatime,size=6G 0 0

tmpfs /dev/shm tmpfs nosuid,nodev,noexec,noatime,size=1G 0 0

dev /dev devtmpfs nosuid,noexec,size=0K 0 0

tmpfs /run tmpfs nosuid,nodev,noexec,size=1G 0 0

proc /proc proc nosuid,nodev,noexec,gid=proc,hidepid=2 0 0
